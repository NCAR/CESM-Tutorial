    "fig, ax = plt.subplots(1, 2, figsize=(12,3), sharex=True, sharey=True)\n",
    "\n",
    "ds_pop.TLAT.plot(ax=ax[0], levels=np.arange(-90,95,5))\n",
    "ax[0].set_title('TLAT')\n",
    "ds_pop.TLONG.plot(ax=ax[1], levels=np.arange(0,370,10))\n",
    "ax[1].set_title('TLONG')"
   ]
